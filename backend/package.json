    "fernet-web": "^1.0.1",
    "i": "^0.3.7",
    "moment": "^2.29.4",
    "npm": "^8.19.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "tinkoff-invest-api": "^5.0.0",
